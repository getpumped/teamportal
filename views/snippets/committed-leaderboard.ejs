<h3>Most Commited</h3>
<table class="table table-condensed">
  <thead>
    <tr>
      <th>#</th>
      <th>Member</th>
      <th>Activities</th>
    </tr>
  </thead>
  <tbody>
    <% for(i=0;i<commitmentLeaderboard.length;i++) { %>
    <tr>
      <td><%= i+1 %></td>
      <td><%= commitmentLeaderboard[i]._id.username %></td>
      <td><%= commitmentLeaderboard[i].value %></td>
    </tr>
    <% } %>
  </tbody>
</table>